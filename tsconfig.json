{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/components/*"],
      "@layouts/*": ["src/layouts/*"],
      "@icons/*": ["src/icons/*"],
      "@lib/*": ["src/lib/*"],
      "@store/*": ["src/store/*"]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}
